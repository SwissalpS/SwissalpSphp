#!/bin/sh
## runCLIwithAltPHP.sh
## set PATH for all scripts run by SssSphocoaApp
## in particular propel
## especially on OSX or other platforms that have more than one version of PHP

SssSAltPHPBASE=`dirname(##PHP_BIN##);`;
## launch app in interactive mode prepending prefered php path to PATH
/usr/bin/env PATH="$SssSAltPHPBASE:$PATH" ##PATH_DIR_PHOCOA_APP####DIR_SEP##bin##DIR_SEP##run -cli;
